{
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "name": "Growntown",
  "scripts": {
    "clean": "rm -rf .dfx && rm -rf src/Growntown_Frontend/dist src/Growntown_AdminFrontend/dist",
    "setup": "npm run clean && npm i && dfx canister create --all && dfx generate && cd tests/Local/./start.sh",
    "start": "npm run start:frontend & npm run start:admin",
    "start:frontend": "cd src/Growntown_Frontend && vite --port 3000",
    "start:admin": "cd src/Growntown_AdminFrontend && vite --port 3001",
    "build": "npm run build --workspaces --if-present"
  },
  "type": "module",
  "workspaces": [
    "src/Growntown_Frontend",
    "src/Growntown_AdminFrontend"
  ],
